EMAIL="your-institutional-email@university.edu"
PASSWORD="your-teams-password"

# PostgreSQL
DB_HOST=localhost
DB_PORT=5433
DB_NAME=backup_teams
DB_USER=your-system-username
DB_PASSWORD=localdev

# Download destination
DOWNLOAD_ROOT=./downloads

# Scraper settings
DOWNLOAD_CONCURRENCY=4

# Fallback metadata
DEFAULT_SEMESTER=2026/1
DEFAULT_YEAR=2026

# ── AWS S3 ─────────────────────────────────────────────────────────────────────
# Create a dedicated IAM user with s3:PutObject, s3:GetObject, s3:HeadObject
# on your bucket. Never use root account keys.
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1
S3_BUCKET=backup-teams-files-rk

# Set to true to delete the local copy after a confirmed S3 upload
DELETE_LOCAL_AFTER_UPLOAD=false
